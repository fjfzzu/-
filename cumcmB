ylabel('Percentage (%)', 'FontName','Times New Roman','FontSize',11,'FontWeight','normal') %y轴坐标描述



%% 时间

Time = [20 70 110 163 197 240 273];

%% 数据
C4_yield = [17.37540804	14.56733047	13.42349545	12.93495022	12.35425379	12.03722565	11.67530575  ];
C4_olefin_selectivity = [39.9	38.55	36.72	39.53	38.96	40.32	39.04];

%% 画图

plot(Time,C4_yield,'-*b',Time,C4_olefin_selectivity,'-or'); %线性，颜色，标记
title('350℃测试结果');
axis( [0,300,0,50])  %确定x轴与y轴框图大小
set(gca,'XTick',[0:50:300]) %x轴范围
set(gca,'YTick',[0:10:50]) %y轴范围
legend('C4 yield','C4 olefin','Location','SouthEast', 'FontName','Times New Roman','FontSize',8,'FontWeight','normal');   %右下角标注
xlabel('Time', 'FontName','Times New Roman','FontSize',11,'FontWeight','normal')  %x轴坐标描述
ylabel('Percentage (%)', 'FontName','Times New Roman','FontSize',11,'FontWeight','normal') %y轴坐标描述
