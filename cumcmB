%% 温度
Tem = 250:25:350;
Tem1 = [250 275 300 325 350 400];
Tem2 = [250 275 300  350 400];
%% 数据
Ethanol_conversion = [4.4	7.9	11.7	17.8	30.2	69.4 ];
C4_olefin_selectivity = [4.08	6.62	12.86	18.45	25.05	38.17];

%% 画图

plot(Tem1,Ethanol_conversion,'-*b',Tem1,C4_olefin_selectivity,'-or'); %线性，颜色，标记
title('B7催化剂组合');
axis( [250,400,0,100])  %确定x轴与y轴框图大小
set(gca,'XTick',[250:25:400]) %x轴范围
set(gca,'YTick',[0:10:100]) %y轴范围
